{
  "name": "phonogram",
  "version": "0.0.1",
  "description": "Collection of phonetic algorithms targeting a variety of European languages.",
  "main": "index.js",
  "scripts": {
    "check": "npm test && npm run lint",
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel-core/register -R spec ./test/endpoint.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yomguithereal/phonogram.git"
  },
  "keywords": [
    "phonetic"
  ],
  "author": {
    "name": "Guillaume Plique",
    "url": "http://github.com/Yomguithereal"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yomguithereal/phonogram/issues"
  },
  "homepage": "https://github.com/yomguithereal/phonogram#readme",
  "devDependencies": {
    "@yomguithereal/eslint-config": "^2.1.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-plugin-transform-es2015-destructuring": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.4.0",
    "mocha": "^2.4.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-es2015-destructuring",
        {
          "loose": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@yomguithereal/eslint-config/es6"
  }
}
